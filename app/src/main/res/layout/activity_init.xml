<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/init10801920"
    tools:context="biezhi.videoplayer.InitActivity">
    <com.wang.avi.AVLoadingIndicatorView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/av_loading"
        android:layout_above="@+id/tv_setting"
        android:layout_centerHorizontal="true"
        app:indicator="LineSpinFadeLoader"
        app:indicator_color="#FFFFFF"
        android:visibility="invisible"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/settingApp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:paddingBottom="25dp"
        android:textSize="15sp"
        android:id="@+id/tv_setting"
        android:visibility="invisible"/>
</RelativeLayout>
